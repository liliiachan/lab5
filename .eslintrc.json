{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-alert": "off",
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-plusplus": "off",
    "camelcase": "off"
  }
}
